        </control>
        <control type="image" id="58001">
            <description>IMAGE_FULL</description>
            <posx>0</posx>
            <posy>0</posy>
            <width>1280</width>
            <height>720</height>
            <label>-</label>
            <texture>$INFO[Window(10000).Property(muplo_image_full)]</texture>
            <visible>!isEmpty(Window(10000).Property(muplo_image_full))</visible>
            <aspectratio>keep</aspectratio>
            <animation effect="fade" time="1000">Visible</animation>
        </control>
    </include>
</includes>